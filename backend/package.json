{
  "name": "voltride-backend",
  "version": "1.0.0",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "start:dev": "nest start --watch",
    "seed": "ts-node --transpile-only src/seed.ts",
    "dev": "ts-node-dev --respawn src/interfaces/server.ts",
    "start": "node dist/interfaces/server.js",
    "prisma:generate:postgres": "prisma generate --schema prisma/postgres/schema.prisma",
    "prisma:migrate:postgres": "prisma migrate dev --schema prisma/postgres/schema.prisma --name",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.10",
    "@nestjs/core": "^11.0.10",
    "@nestjs/platform-express": "^11.0.10",
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "mongoose": "^8.10.2",
    "nodemon": "^3.0.1",
    "prettier": "3.4.2",
    "prisma": "^6.4.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
